package it.cle.project.dto.fattura;

import java.io.Serializable;
import java.util.Date;

import javax.xml.bind.annotation.XmlRootElement;
@XmlRootElement
public class DatiGeneraliDocumentoDTO implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String tipoDocumento;
	private String divisa;
	private Date data; // YYY-MM-DD
	private String numero;
	private DatiRitenutaDTO datiRitenuta;
	private DatiBolloDTO datiBollo;
	private DatiCassaPrevidenzialeDTO datiCassaPrevidenziale;
	private ScontoDTO scontoMaggiorazione;
	private Double importoTotaleDocumento;
	private Double arrotondamento;
	private String causale;
	private String art73;
	
	/**
	 * @return the tipoDocumento
	 */
	public String getTipoDocumento() {
		return tipoDocumento;
	}
	/**
	 * @param tipoDocumento the tipoDocumento to set
	 */
	public void setTipoDocumento(String tipoDocumento) {
		this.tipoDocumento = tipoDocumento;
	}
	/**
	 * @return the divisa
	 */
	public String getDivisa() {
		return divisa;
	}
	/**
	 * @param divisa the divisa to set
	 */
	public void setDivisa(String divisa) {
		this.divisa = divisa;
	}
	/**
	 * @return the data
	 */
	public Date getData() {
		return data;
	}
	/**
	 * @param data the data to set
	 */
	public void setData(Date data) {
		this.data = data;
	}
	/**
	 * @return the numero
	 */
	public String getNumero() {
		return numero;
	}
	/**
	 * @param numero the numero to set
	 */
	public void setNumero(String numero) {
		this.numero = numero;
	}
	/**
	 * @return the datiRitenuta
	 */
	public DatiRitenutaDTO getDatiRitenuta() {
		return datiRitenuta;
	}
	/**
	 * @param datiRitenuta the datiRitenuta to set
	 */
	public void setDatiRitenuta(DatiRitenutaDTO datiRitenuta) {
		this.datiRitenuta = datiRitenuta;
	}
	
	
	

}
