package it.cle.project.dto.fattura;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class AnagraficaDTO implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String denominazione;
	private String nome;
	private String cognome;
	private String titolo;
	private String codEORI ;
	/**
	 * @return the denominazione
	 */
	public String getDenominazione() {
		return denominazione;
	}
	/**
	 * @param denominazione the denominazione to set
	 */
	@XmlElement(name="denominazione")
	public void setDenominazione(String denominazione) {
		this.denominazione = denominazione;
	}
	/**
	 * @return the nome
	 */
	public String getNome() {
		return nome;
	}
	/**
	 * @param nome the nome to set
	 */
	@XmlElement(name="nome")
	public void setNome(String nome) {
		this.nome = nome;
	}
	/**
	 * @return the cognome
	 */
	
	public String getCognome() {
		return cognome;
	}
	/**
	 * @param cognome the cognome to set
	 */
	@XmlElement(name="cognome")
	public void setCognome(String cognome) {
		this.cognome = cognome;
	}
	/**
	 * @return the titolo
	 */
	public String getTitolo() {
		return titolo;
	}
	/**
	 * @param titolo the titolo to set
	 */
	@XmlElement(name="titolo")
	public void setTitolo(String titolo) {
		this.titolo = titolo;
	}
	/**
	 * @return the codEORI
	 */
	public String getCodEORI() {
		return codEORI;
	}
	/**
	 * @param codEORI the codEORI to set
	 */
	@XmlElement(name="codEORI")
	public void setCodEORI(String codEORI) {
		this.codEORI = codEORI;
	}
	
	

}
