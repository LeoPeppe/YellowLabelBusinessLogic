package it.cle.project.dto.fattura;

import java.io.Serializable;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class SedeDTO implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String indirizzo;
	private String numeroCivico;
	private String cap;
	private String comune;
	private String provincia;
	private String nazione;
	/**
	 * @return the indirizzo
	 */
	public String getIndirizzo() {
		return indirizzo;
	}
	/**
	 * @param indirizzo the indirizzo to set
	 */
	@XmlElement(name="indirizzo")
	public void setIndirizzo(String indirizzo) {
		this.indirizzo = indirizzo;
	}
	/**
	 * @return the numeroCivico
	 */
	public String getNumeroCivico() {
		return numeroCivico;
	}
	/**
	 * @param numeroCivico the numeroCivico to set
	 */
	@XmlElement(name="numeroCivico")
	public void setNumeroCivico(String numeroCivico) {
		this.numeroCivico = numeroCivico;
	}
	/**
	 * @return the cap
	 */
	public String getCap() {
		return cap;
	}
	/**
	 * @param cap the cap to set
	 */
	@XmlElement(name="CAP")
	public void setCap(String cap) {
		this.cap = cap;
	}
	/**
	 * @return the comune
	 */
	public String getComune() {
		return comune;
	}
	/**
	 * @param comune the comune to set
	 */
	@XmlElement(name="comune")
	public void setComune(String comune) {
		this.comune = comune;
	}
	/**
	 * @return the provincia
	 */
	public String getProvincia() {
		return provincia;
	}
	/**
	 * @param provincia the provincia to set
	 */
	@XmlElement(name="provincia")
	public void setProvincia(String provincia) {
		this.provincia = provincia;
	}
	/**
	 * @return the nazione
	 */
	public String getNazione() {
		return nazione;
	}
	/**
	 * @param nazione the nazione to set
	 */
	@XmlElement(name="nazione")
	public void setNazione(String nazione) {
		this.nazione = nazione;
	}
	
	

}
